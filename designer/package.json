{
  "scripts": {
    "postcss:watch": "postcss src/assets/css/index.pcss -o src/assets/css/index.css --env development -w",
    "postcss:build": "postcss src/assets/css/index.pcss -o src/assets/css/index.css --env production",
    "parcel:serve": "parcel src/index.html",
    "parcel:watch": "parcel watch src/index.html",
    "parcel:build": "parcel build src/index.html",
    "clean": "rm -rf dist .cache",
    "build": "npm-run-all -s clean postcss:build parcel:build",
    "start": "npm-run-all -s clean -p postcss:watch parcel:serve",
    "dev": "sst-env -- npm run start"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.0.1",
    "@parcel/transformer-webmanifest": "^2.0.1",
    "@serverless-stack/static-site-env": "^0.53.0",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.8",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.1",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.1",
    "tailwindcss": "^3.0.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@themesberg/flowbite": "^1.1.1",
    "@types/fabric": "^4.5.4",
    "axios": "^0.24.0",
    "fabric": "^4.6.0",
    "fast-average-color": "^7.0.1",
    "nouislider": "^15.5.0",
    "tinycolor2": "^1.4.2"
  }
}
